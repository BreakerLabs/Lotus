fn foo(mut x: i32, y: f64) {
    x = 86;
    // y = 5.34; -> gives error
    printf("x: %d, y: %f\n", x, y);
}

fn main() {
    x: i32 = 10;
    // x = 5; -> gives error

    mut y: i32 = 45;
    y = 5;

    a: *i32 = &x;
    // *a = 5; -> gives error

    b: *mut i32 = &mut y;
    *b = 34;

    printf("%d, %d\n", x, y);

    foo(123, 345.4);
}

// output:
// 10, 34
// x: 86, y: 345.400000
