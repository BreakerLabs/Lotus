fn sum(x: i32, y: i32) -> i32 {
    ret x + y;
}

fn main() -> i32 {
    if 2 > 3 {
        printf("9");
    } else if 7 - 2 == 5 && 4 >= 2 {
        if not 2 + 1 - 3: printf("0");
        printf("5");
    } else if 3 - 2 {
        printf("6");
    } else {
        printf("8");
    }
    ret sum(7, -2);
}

