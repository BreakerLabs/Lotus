fn plotBooleanGrid(arr1: [bool|5], arr2: [bool|5]) {
    for mut i := 0; i < 5; i++ {
        for mut j := 0; j < 5; j++ {
            if arr1[i] && arr2[j] {
                printf("# ");
            } else if !arr1[i] && !arr2[j] {
                printf("  ");
            } else {
                printf("* ");
            }
        }
        printf("\n");
    }
}

fn main() {
    arr1: [bool|5] = bool[1, 0, 1, 1, 0];
    arr2: [bool|5] = bool[0, 1, 0, 1, 1];
    plotBooleanGrid(arr1, arr2);
}

// output:
// * # * # #
//   *   * *
// * # * # #
// * # * # #
//   *   * *